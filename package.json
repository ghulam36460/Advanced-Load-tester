{
  "name": "advanced-load-tester",
  "version": "2.0.0",
  "description": "Next-Generation Load Testing Framework with ML/AI-powered Python integration and Real-time Analytics",
  "main": "app.js",
  "scripts": {
    "start": "node app.js",
    "dev": "nodemon app.js --watch js --watch dashboard --ext js,html,css",
    "start-js-only": "node js/index.js",
    "start-python-only": "python python_bridge_new.py",
    "test": "vitest run --coverage --reporter=verbose",
    "test:jest": "jest --coverage",
    "cleanup-duplicates": "powershell -ExecutionPolicy Bypass -File .\\cleanup-duplicates.ps1",
    "test:unit": "jest tests/unit",
    "test:integration": "jest tests/integration",
    "lint": "eslint js/**/*.js app.js",
    "lint:fix": "eslint js/**/*.js app.js --fix",
    "format": "prettier --write js/**/*.js app.js dashboard/**/*.{html,css,js}",
    "build": "webpack --mode production",
    "build:dev": "webpack --mode development --watch",
    "dashboard": "node app.js",
    "install-python": "pip install -r requirements.txt",
    "install-all": "npm install && npm run install-python",
    "setup": "npm run install-all",
    "setup-dev": "npm run install-all && npm run setup-git-hooks",
    "setup-git-hooks": "npx husky install",
    "clean": "rimraf logs/ node_modules/ dist/ coverage/",
    "health-check": "curl -f http://localhost:3000/health || exit 1",
    "stress-test": "node scripts/stress-test.js",
    "benchmark": "node scripts/benchmark.js",
    "security-scan": "npm audit --audit-level=moderate",
    "docker:build": "docker build -t advanced-load-tester .",
    "docker:run": "docker run -p 3000:3000 -p 5001:5001 advanced-load-tester",
  "dashboard:config": "node scripts/write-dashboard-config.js",
    "docs:generate": "jsdoc js/ -d docs/",
    "release": "npm version patch && git push && git push --tags"
  },
  "keywords": [
    "load-testing",
    "performance-testing",
    "stress-testing",
    "nodejs",
    "python",
    "machine-learning",
    "ai-optimization",
    "real-time-monitoring",
    "websocket",
    "graphql",
    "grpc",
    "browser-automation",
    "analytics",
    "dashboard",
    "microservices",
    "devops"
  ],
  "author": "Advanced Load Tester Team",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.5.0",
    "bcrypt": "^5.1.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "helmet": "^7.0.0",
    "https-proxy-agent": "^7.0.2",
    "joi": "^17.9.2",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "morgan": "^1.10.0",
    "proxy-agent": "^6.3.1",
    "socket.io": "^4.7.2",
    "socket.io-client": "^4.7.2",
    "socks-proxy-agent": "^8.0.2",
    "uuid": "^9.0.0",
    "winston": "^3.10.0",
    "ws": "^8.13.0"
  },
  "devDependencies": {
  "@eslint/js": "^9.13.0",
    "@types/node": "^20.5.0",
    "@vitest/coverage-v8": "^2.1.9",
    "eslint": "^9.0.0",
    "husky": "^8.0.3",
    "jest": "^30.0.0",
    "jsdoc": "^4.0.2",
    "lint-staged": "^16.1.5",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.2",
    "rimraf": "^5.0.1",
    "supertest": "^7.1.3",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6",
    "vitest": "^2.1.9",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4"
  },
  "optionalDependencies": {
    "@apollo/client": "^3.8.0",
    "@grpc/grpc-js": "^1.9.0",
    "@grpc/proto-loader": "^0.7.8",
    "amqplib": "^0.10.3",
    "artillery": "^1.3.7",
    "autocannon": "^7.12.0",
    "benchmark": "^2.1.4",
    "clinic": "^9.1.0",
    "faker": "^5.5.3",
    "graphql": "^16.8.0",
    "k6": "^0.46.0",
    "kafka-node": "^5.0.0",
    "mqtt": "^5.0.0",
    "playwright": "^1.37.0",
    "puppeteer": "^24.16.2",
    "redis": "^4.6.8"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0",
    "python": ">=3.8.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/advanced-load-tester.git"
  },
  "bugs": {
    "url": "https://github.com/your-org/advanced-load-tester/issues"
  },
  "homepage": "https://github.com/your-org/advanced-load-tester#readme",
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{html,css,md}": [
      "prettier --write"
    ]
  },
  "jest": {
    "testEnvironment": "node",
    "testMatch": [
      "**/__tests__/**/*.js?(x)",
      "**/?(*.)+(spec|test).js?(x)",
      "**/test_*.js"
    ],
    "collectCoverageFrom": [
      "js/**/*.js",
      "app.js",
      "!js/**/*.test.js",
      "!js/node_modules/**"
    ],
    "coverageDirectory": "coverage",
    "coverageReporters": [
      "text",
      "lcov",
      "html"
    ]
  }
}
